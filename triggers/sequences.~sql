CREATE SEQUENCE funkcio_seq
MINVALUE 1 MAXVALUE 9999
START WITH 1
INCREMENT BY 1;

/*Audit táblák szekvenciái*/
create sequence funkcio_irodaiszek_audit_seq start with 1 INCREMENT BY 1;
create sequence rendeles_irodaiszek_audit_seq start with 1 INCREMENT BY 1;

/*Táblákhoz tartozó*/
create sequence irodaiszek_seq start with 1 INCREMENT BY 1;
create sequence funkcionalitas_seq start with 1 INCREMENT BY 1;
create sequence hattamla_seq start with 1 INCREMENT BY 1;
create sequence rendeles_seq start with 1 INCREMENT BY 1;
create sequence talp_seq start with 1 INCREMENT BY 1;
create sequence ules_seq start with 1 INCREMENT BY 1;
