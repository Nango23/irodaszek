PROMPT Creating talp...

CREATE TABLE rendeles_irodaiszek(
rendeles_id                NUMBER        NOT NULL FOREIGN KEY REFERENCES rendeles(rendeles_id)
,irodaiszek_id             NUMBER        NOT NULL FOREIGN KEY REFERENCES irodaiszek(irodaiszek_id)
,last_modified             DATE          DEFAULT SYSDATE NOT NULL
,created                   DATE          DEFAULT SYSDATE NOT NULL
);

COMMENT ON TABLE rendeles_irodaiszek IS 'Rendelésekhez tartozó irodai székek';
COMMENT ON COLUMN rendeles_irodaiszek.rendeles_id IS 'Rendelés azonosítója';
COMMENT ON COLUMN rendeles_irodaiszek.irodaiszek_kód IS 'Irodaiszék azonosítója';

